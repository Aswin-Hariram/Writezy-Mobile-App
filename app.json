{
  "expo": {
    "name": "Writezy",
    "slug": "Writezy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aswin_hariram.Writezy"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/Fonts/Delius-Regular.ttf",
            "./assets/Fonts/Lexend-VariableFont_wght.ttf",
            "./assets/Fonts/Zain-Black.ttf",
            "./assets/Fonts/Zain-Italic.ttf",
            "./assets/Fonts/Zain-Regular.ttf"
          ]
        }
      ]
    ]
  }
}
